program = {
  name = grub-emu;
  mansection = 1;

  source = kern/emu/full.c;
  nodist = grub_emu_init.c;

  ldadd = 'kernel.img$(EXEEXT)';
  ldadd = '$(MODULE_FILES)';
  ldadd = '$(LIBCURSES) $(LIBSDL) $(LIBUSB) $(LIBPCIACCESS)';

  enable = emu;
};

